<p id="notice"><%= notice %></p>

<h1>Task</h1>


<table>
  <head>
    <tr>
      <th>Task_name</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </head>

  <body>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= task.task_name %></td>
        <td><%= task.content %></td>
        <td><%= link_to 'Show', task %></td>
        <td><%= link_to 'Edit', edit_task_path(task) %></td>
        <td><%= link_to 'Destroy', task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </body>
</table>

<br>

<%= link_to 'New task', new_task_path %>
